<script>
  function jumpTo(key, val){
    const currentUrl = new URL(window.location.href);
    currentUrl.searchParams.set(key, val);
    Turbo.visit(currentUrl.toString());
  }

  function jumpToAyah(selectAyah) {
    jumpTo('ayah', selectAyah);
  }

  function jumpToSurah(selectSurah) {
    jumpTo('surah', selectSurah);
  }

  function jumpToPage(selectedPage) {
    jumpTo('page', selectedPage);
  }
</script>

